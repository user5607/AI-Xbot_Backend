{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ES2020",
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": false,
    "strict": false,
    "skipLibCheck": true,
    "allowJs": true,
    "noEmit": true
  },
  "include": ["./**/*"],
  "exclude": ["node_modules", ".wrangler"]
}